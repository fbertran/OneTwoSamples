trace("gene_expr_simulation", signature="network", browser, exit=browser)
untrace("gene_expr_simulation", signature="network")
trace("inference", signature="micro_array", browser, exit=browser, where = asNamespace("Patterns"))
trace("predict", signature="micro_array,network", browser, exit=browser, where = asNamespace("Patterns"))
trace("predict", signature="micro_array", browser, exit=browser, where = asNamespace("Patterns"))
trace("predict", signature="ANY,network", browser, exit=browser, where = asNamespace("Patterns"))
trace("predict", signature="ANY", browser, exit=browser, where = asNamespace("Patterns"))
trace("predict", signature="ANY,ANY", browser, exit=browser, where = asNamespace("Patterns"))

trace("plot", signature="network", browser, exit=browser, where = asNamespace("Patterns"))


